<% include ../partials/header %>
<table class="table">
    <thead>
        <tr>
            <th>sid</th>
            <th>title</th>
            <th>firstname</th>
            <th>lastname</th>
        </tr>
    </thead>
    <tbody>
        <% products.forEach(function(product){%>
            <tr>
                <td scope="row">
                    <%= product.id%>
                </td>
                <td>
                    <%= product.title%>
                </td>
                <td>
                    <%= product.price%>
                </td>
            </tr>
            <%}); %>
    </tbody>
</table>


<% include ../partials/footer %>